<% layout('layout/boilerplate') %>

<div class="container">
    <h1>Staff</h1>
    <table class="table table-hover">
        <thead>
          <tr>
            <th>Staff ID</th>
            <th>Name</th>
            <th>Position</th>
          </tr>
        </thead>
        <tbody>
          <% for (let staff of staffs) { %>
            <tr>
              <td><%= staff.id %></td>
              <td><%= staff.name %></td>
              <td><%= staff.position %></td>
            </tr>
          <% } %>
        </tbody>
      </table>
</div>
